			function weibo(){
				var _url="http://v.t.sina.com.cn/share/share.php?&searchPic=false";
				_url+=("&url="+encodeURI(sns_url));
				_url+=("&title="+encodeURI(sns_text));
				//_url+="&pic=http://"+location.host+"/img/share.jpg";
				//hard code pic url
				_url+="&pic=http://c-class.mymb.com.cn/img/share.jpg";
				window.open(_url,"_blank");
			}
			function tencent(){
				var _url="http://v.t.qq.com/share/share.php?";
				_url+=("&url="+encodeURI(sns_url));
				_url+=("&title="+encodeURI(sns_text));
				//_url+="&pic=http://"+location.host+"img/share.jpg";
				//hard code pic url
				_url+="&pic=http://c-class.mymb.com.cn/img/share.jpg";

				window.open(_url,"_blank");
			}
			function renren(){
				var _url="http://widget.renren.com/dialog/share?charset=UTF-8";
				_url+=("&resourceUrl="+encodeURI(sns_url));
				_url+=("&srcUrl="+encodeURI(sns_url));
				_url+=("&title="+encodeURI(sns_title));
				_url+=("&description="+encodeURI(sns_text));				
				//_url+="&pic=http://"+location.host+"img/share.jpg";
				//hard code pic url
				_url+="&pic=http://c-class.mymb.com.cn/img/share.jpg";
				window.open(_url,"_blank");
			}

